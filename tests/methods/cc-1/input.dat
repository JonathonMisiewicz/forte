#! RHF-CCSD 6-31G** all-electron optimization of the H2O molecule
import forte

molecule h2o {
    O
    H 1 1.1
    H 1 1.1 2 104.0
symmetry c1
}

set {
  basis DZ
  scf_type           pk
  reference          rhf
  e_convergence      10
  d_convergence      10
  r_convergence      10
#  docc               [3,0,1,1]
}

set forte {
  job_type               cc
  int_type               conventional
#  restricted_docc        [3,0,1,1]
#  active                 [0,0,0,0]
  restricted_docc        [5]
  active                 [0]
  e_convergence          10
  r_convergence          10
  maxiter                100
}

Ehf, wfn = energy('scf', return_wfn=True)
E_ccsd = energy('ccsd', ref_wfn = wfn)

energy('forte', ref_wfn = wfn)
compare_values(E_ccsd, get_variable("CURRENT ENERGY"), 10, "CCSD Energy") #TEST
